{
  // Configure your Python interpreter path here
  // Examples:
  //   "python.defaultInterpreterPath": "${workspaceFolder}/dashboard/backend/.venv/Scripts/python.exe",
  //   "python.defaultInterpreterPath": "${workspaceFolder}/.venv/Scripts/python.exe",
  //   "python.defaultInterpreterPath": "python",

  // MCP Servers for Claude VS Code extension
  // Documentation search server - requires Weaviate running with ingested docs
  "claude.mcpServers": {
    "documentation": {
      "command": "python",
      "args": ["-m", "mcp_servers.documentation.main"],
      "cwd": "${workspaceFolder}"
    }
  },

  "files.watcherExclude": {
    "**/alltalk_tts/**": true,
    "**/audiocraft/**": true,
    "**/ComfyUI/**": true,
    "**/DiffRhythm/**": true,
    "**/local-talking-llm/**": true,
    "**/MusicGPT/**": true,
    "**/stable-audio-tools/**": true,
    "**/stability-matrix/**": true,
    "**/Wan2GP/**": true,
    "**/YuE/**": true,
    "**/node_modules/**": true,
    "**/ollama-models/**": true,
    "**/__pycache__/**": true,
    "**/.git/**": true
  },
  "files.exclude": {
    "alltalk_tts": true,
    "audiocraft": true,
    "ComfyUI": true,
    "DiffRhythm": true,
    "local-talking-llm": true,
    "MusicGPT": true,
    "stable-audio-tools": true,
    "stability-matrix": true,
    "Wan2GP": true,
    "YuE": true,
    "node_modules": true,
    "ollama-models": true,
    "__pycache__": true
  },
  "search.exclude": {
    "**/alltalk_tts/**": true,
    "**/audiocraft/**": true,
    "**/ComfyUI/**": true,
    "**/DiffRhythm/**": true,
    "**/local-talking-llm/**": true,
    "**/MusicGPT/**": true,
    "**/stable-audio-tools/**": true,
    "**/stability-matrix/**": true,
    "**/Wan2GP/**": true,
    "**/YuE/**": true,
    "**/node_modules/**": true,
    "**/ollama-models/**": true
  },
  "git.ignoredRepositories": [
    "alltalk_tts",
    "audiocraft",
    "ComfyUI",
    "DiffRhythm",
    "local-talking-llm",
    "MusicGPT",
    "stable-audio-tools",
    "stability-matrix",
    "Wan2GP",
    "YuE"
  ]
}
