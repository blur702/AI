# Brevo MCP Token (API Key)
# Get from: Brevo Account Settings > API Keys > Create MCP server API key
BREVO_MCP_TOKEN=your_brevo_mcp_token_here

# Brevo MCP Base URL (default: https://mcp.brevo.com)
BREVO_MCP_BASE_URL=https://mcp.brevo.com

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
